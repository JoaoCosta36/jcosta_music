{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static",
        "config": {
          "distDir": "frontend/dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }